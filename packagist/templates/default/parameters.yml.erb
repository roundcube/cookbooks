parameters:
    database_driver: pdo_mysql
    database_host: 127.0.0.1
    database_name: <%=@db["db_name"]%>
    database_user: root
    database_password:

    mailer_transport:
    mailer_host: <%=@smtp["hostname"]%>
    mailer_user: <%=@smtp["username"]%>
    mailer_password: <%=@smtp["password"]%>
    mailer_from_email: <%=@email%>
    mailer_from_name: <%=@server_name%>

    packagist_host: <%=@server_name%>
    # router.request_context.host: %packagist_host%
    # router.request_context.scheme: https

    redis_dsn: redis://127.0.0.1/1

    locale: en

    secret: <%=@secret%>
    remember_me.secret: <%=@remember_me%>

    google_analytics:
        ga_key:

    github.client_id: <%=@github_client%>
    github.client_secret: <%=@github_secret%>

    force_ssl: false
